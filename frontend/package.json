{
   "name": "nodejs-messaging-work-queue-frontend",
   "version": "4.0.1",
   "description": "A sample Node.js app that sends requests",
   "license": "Apache-2.0",
   "scripts": {
      "test": "tape test/*.js | tap-spec",
      "lint": "xo",
      "build": "npm run copy:patternfly-assets",
      "copy:patternfly-assets": "cp -R node_modules/@patternfly/patternfly/assets public && cp node_modules/@patternfly/patternfly/patternfly.css public/patternfly.css && cp node_modules/@patternfly/patternfly/patternfly-addons.css public/patternfly-addons.css",
      "coverage": "nyc npm test",
      "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
      "ci": "npm run lint && npm run coveralls",
      "dependencyCheck": "szero . --ci",
      "prepublish": "license-reporter report",
      "openshift": "nodeshift --strictSSL=false --nodeVersion=10.x",
      "postinstall": "npm run copy:patternfly-assets && license-reporter report -s && license-reporter save -s --xml licenses.xml",
      "start": "node .",
      "start:dev": "nodemon ."
   },
   "main": "./bin/www",
   "xo": {
      "space": 2,
      "rules": {
         "space-before-function-paren": [
            "error",
            "always"
         ],
         "camelcase": [
            "off"
         ]
      },
      "ignores": [
         "public/**"
      ]
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/bucharest-gold/nodejs-messaging-work-queue.git"
   },
   "files": [
      "package.json",
      "app.js",
      "public",
      "bin",
      "LICENSE",
      "licenses"
   ],
   "bugs": {
      "url": "https://github.com/bucharest-gold/nodejs-messaging-work-queue/issues"
   },
   "homepage": "https://github.com/bucharest-gold/nodejs-messaging-work-queue",
   "dependencies": {
      "@patternfly/patternfly": "~2.33.5",
      "body-parser": "~1.18.3",
      "debug": "~3.1.0",
      "env-var": "~5.1.0",
      "express": "^4.16.0",
      "express-handlebars": "~3.1.0",
      "express-session": "~1.16.2",
      "handlebars": "~4.4.0",
      "kube-probe": "^0.3.1",
      "license-reporter": "~1.2.1",
      "rhea": "^0.2.13"
   },
   "devDependencies": {
      "nodemon": "~1.19.3",
      "nodeshift": "~1.11.0",
      "szero": "~1.0.1",
      "xo": "~0.21.1"
   }
}
